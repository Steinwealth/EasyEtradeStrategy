# === Development Environment Overrides ===
# Development-specific settings for testing and debugging
# Last updated: 2025-01-27

# === ENVIRONMENT ===
ENVIRONMENT=development
DEBUG_MODE=true
MOCK_DATA=false

# === DEVELOPMENT SETTINGS ===
LOG_LEVEL=DEBUG
SESSION_LOG_LEVEL=DEBUG
LOG_DATA_CALLS=true
LOG_CALL_USAGE=true
LOG_LATENCY_METRICS=true
LOG_PROVIDER_SWITCHES=true
LOG_CACHE_HITS=true

# === DEVELOPMENT DATA PROVIDERS ===
ETRADE_SANDBOX=true
POLYGON_QPM_BUDGET=100
ALPHA_VANTAGE_DAILY_LIMIT=100

# === DEVELOPMENT PERFORMANCE ===
MAX_WORKERS=2
BATCH_SIZE=5
CACHE_TTL_SECONDS=10

# === DEVELOPMENT ALERTING ===
TELEGRAM_CHAT_ID=375360269
ALERT_ON_ERRORS=true
ALERT_ON_DATA_FAILURES=true

# === DEVELOPMENT MONITORING ===
MONITOR_CALL_USAGE=true
MONITOR_LATENCY=true
MONITOR_PROVIDER_STATUS=true
MONITOR_CACHE_EFFICIENCY=true

# === DEVELOPMENT SAFETY ===
AUTOMATION_MODE=off
NO_LIVE_TRADING=true
SIMULATION_MODE=true