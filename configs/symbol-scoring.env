# Symbol Scoring Configuration
# Prime Symbol Score System Settings

# Data Storage
SYMBOL_SCORE_DATA_FILE=data/symbol_scores.json
SYMBOL_SCORE_BACKUP_INTERVAL=100
SYMBOL_SCORE_BACKUP_RETENTION_DAYS=30

# Performance Tracking
SYMBOL_SCORE_MAX_TRADES_PER_SYMBOL=300
SYMBOL_SCORE_MIN_TRADES_FOR_RANKING=5
SYMBOL_SCORE_MIN_TRADES_FOR_ANALYSIS=3

# Priority Calculation
SYMBOL_SCORE_MAX_WEIGHT_MULTIPLIER=2.0
SYMBOL_SCORE_MIN_WEIGHT_MULTIPLIER=0.5
SYMBOL_SCORE_EXCELLENT_THRESHOLD=10.0
SYMBOL_SCORE_GOOD_THRESHOLD=5.0
SYMBOL_SCORE_AVERAGE_THRESHOLD=0.0

# Position Sizing Integration
SYMBOL_SCORE_ENABLE_POSITION_BOOSTING=true
SYMBOL_SCORE_MAX_POSITION_INCREASE=1.5
SYMBOL_SCORE_MIN_POSITION_DECREASE=0.8
SYMBOL_SCORE_CONFIDENCE_BOOST_ENABLED=true
SYMBOL_SCORE_MAX_CONFIDENCE_BOOST=1.5

# Reporting
SYMBOL_SCORE_DAILY_REPORT_ENABLED=true
SYMBOL_SCORE_TOP_N_SYMBOLS=10
SYMBOL_SCORE_EXPORT_FORMAT=csv
SYMBOL_SCORE_REPORT_RETENTION_DAYS=90

# Data Cleanup
SYMBOL_SCORE_CLEANUP_ENABLED=true
SYMBOL_SCORE_CLEANUP_INTERVAL_HOURS=24
SYMBOL_SCORE_DATA_RETENTION_DAYS=90

# Integration Settings
SYMBOL_SCORE_INTEGRATION_ENABLED=true
SYMBOL_SCORE_AUTO_SAVE_ENABLED=true
SYMBOL_SCORE_AUTO_SAVE_INTERVAL_MINUTES=30

# Performance Thresholds
SYMBOL_SCORE_EXCELLENT_WIN_RATE=80.0
SYMBOL_SCORE_GOOD_WIN_RATE=70.0
SYMBOL_SCORE_AVERAGE_WIN_RATE=60.0

# Volatility Analysis
SYMBOL_SCORE_VOLATILITY_THRESHOLD=5.0
SYMBOL_SCORE_HIGH_VOLATILITY_THRESHOLD=10.0

# Strategy Mode Weights
SYMBOL_SCORE_STANDARD_WEIGHT=1.0
SYMBOL_SCORE_ADVANCED_WEIGHT=1.1
SYMBOL_SCORE_QUANTUM_WEIGHT=1.2

# Confidence Integration
SYMBOL_SCORE_CONFIDENCE_THRESHOLD=0.9
SYMBOL_SCORE_ULTRA_HIGH_CONFIDENCE_THRESHOLD=0.995
SYMBOL_SCORE_HIGH_CONFIDENCE_THRESHOLD=0.95

# Alert Settings
SYMBOL_SCORE_ALERT_TOP_PERFORMERS=true
SYMBOL_SCORE_ALERT_POOR_PERFORMERS=true
SYMBOL_SCORE_ALERT_THRESHOLD_CHANGE=2.0
SYMBOL_SCORE_ALERT_WIN_RATE_DROP=10.0

# Database Settings (if using database)
SYMBOL_SCORE_USE_DATABASE=false
SYMBOL_SCORE_DATABASE_URL=sqlite:///data/symbol_scores.db
SYMBOL_SCORE_DATABASE_POOL_SIZE=10

# Logging
SYMBOL_SCORE_LOG_LEVEL=INFO
SYMBOL_SCORE_LOG_TRADES=true
SYMBOL_SCORE_LOG_RANK_UPDATES=true
SYMBOL_SCORE_LOG_CLEANUP=true

# Testing
SYMBOL_SCORE_TEST_MODE=false
SYMBOL_SCORE_MOCK_DATA=false
SYMBOL_SCORE_SIMULATION_MODE=false
